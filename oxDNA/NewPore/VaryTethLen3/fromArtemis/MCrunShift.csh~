#!/bin/sh
pwd


set oxPath = $argv[1]

set lval    = $argv[2]

set simPath = $oxPath/NewPore/VaryTethLen3/

cd $simPath

switch ($lval) 
    case 1: 
	set x = 15
	breaksw
    case 2: 
	set x = 30
	breaksw
    case 3: 
	set x = 45
	breaksw
    case 4: 
	set x = 60
	breaksw
    case 5: 
	set x = 75
	breaksw
    case 6: 
	set x = 90
	breaksw
    case 7: 
	set x = 105
	breaksw	
    case 8: 
	set x = 120
	breaksw		
endsw



foreach l ( $x ) 
    cd len$l
    cd shiftSeq700
    cp $simPath/inputMC .
    pwd    
    $oxPath/oxDNA inputMC

    cd ..
    cd tethAllT
    cp $simPath/inputMC .
    pwd
    $oxPath/oxDNA inputMC

    cd ../..
end

